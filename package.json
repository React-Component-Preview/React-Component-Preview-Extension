{
  "name": "react-component-preview",
  "displayName": "React Component Preview",
  "description": "VScode extension for React component preview",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:react-preview.start",
    "onCommand:react-preview.doRefactor",
    "onWebviewPanel:react-preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-preview.start",
        "title": "Start preview",
        "category": "React Component Preview"
      },
      {
        "command": "react-preview.doRefactor",
        "title": "Do some refactoring",
        "category": "React Component Preview"
      }
    ],
    "keybindings": [
      {
        "command": "react-preview.start",
        "key": "ctrl+p",
        "mac": "cmd+p",
        "when": "editorTextFocus && !editorReadonly && editorLangId == javascript"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  }
}
