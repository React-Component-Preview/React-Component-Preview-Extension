{
  "name": "react-component-preview",
  "displayName": "React Component Preview",
  "version": "0.0.3",
  "publisher": "SeoJunYoo",
  "description": "VScode extension for React component preview.",
  "icon": "icon.png",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Seo Jun Yoo",
    "email": "sytwoyou@gmail.com"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "react component preview",
    "react",
    "preview",
    "component",
    "react preview"
  ],
  "activationEvents": [
    "onCommand:preview.start",
    "onWebviewPanel:react-preview"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "preview.start",
        "title": "Start preview",
        "category": "React Component Preview"
      }
    ],
    "keybindings": [
      {
        "command": "preview.start",
        "key": "ctrl+p",
        "mac": "cmd+p",
        "when": "editorTextFocus && !editorReadonly && editorLangId == javascript || editorLangId == javascriptreact"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode production",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode production",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/src/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.19",
    "@types/vscode": "^1.63.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "eslint-plugin-react": "^7.28.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "webpack": "^5.65.0",
    "webpack-dev-server": "^4.3.0"
  },
  "bugs": {
    "url": "https://github.com/React-Component-Preview/react-preview-extension/issues",
    "email": "sytwoyou@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/React-Component-Preview/react-preview-extension.git"
  }
}
